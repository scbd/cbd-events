{
  "name": "cbd-events",
  "version": "3.0.0",
  "description": "cbd-events app",
  "author": "Secretariat of the Convention on Biological Diversity <it@cbd.int>",
  "private": true,
  "scripts": {
    "dev": "rm -rf .nuxt  &&  NODE_ENV=local nuxt",
    "build:a": "rm -rf .nuxt && NODE_ENV=android nuxt generate && cd capacitor && yarn jetify && yarn cap sync android",
    "build:i": "rm -rf .nuxt && NODE_ENV=ios nuxt generate && cd capacitor && yarn cap sync ios",
    "clean-reinstall": "rm -f yarn.lock && rm -f package-lock.json &&  rm -rf node_modules && CXXFLAGS=\"--std=c++17\" yarn install --force"
  },
  "dependencies": {
    "@awesome-cordova-plugins/core": "6.2.0",
    "@awesome-cordova-plugins/file-opener": "6.2.0",
    "@capacitor/app": "4.1.1",
    "@capacitor/core": "4.5.0",
    "@capacitor/device": "4.1.0",
    "@capacitor/filesystem": "4.1.4",
    "@capacitor/local-notifications": "4.1.3",
    "@capacitor/share": "4.1.0",
    "@capacitor/splash-screen": "4.1.2",
    "@capacitor/status-bar": "4.1.1",
    "@capgo/capacitor-updater": "4.12.8",
    "@ionic-native/core": "5.36.0",
    "@ionic-native/http": "5.36.0",
    "@nuxtjs/axios": "5.13.6",
    "@scbd/conference-cal": "1.0.3",
    "bootstrap": "4.6",
    "camelcase-keys": "8.0.2",
    "cordova-plugin-advanced-http": "3.3.1",
    "localforage": "1.10.0",
    "lodash.debounce": "4.0.8",
    "luxon": "3.1.0",
    "nuxt": "2.15.8",
    "nuxt-i18n": "6.28.1",
    "object-sizeof": "1.6.3",
    "semver": "7.3.8",
    "sweetalert2": "11.6.8",
    "velocity-animate": "1.5.2",
    "vue-cordova": "0.1.2",
    "vue-notifications": "1.0.2"
  },
  "devDependencies": {
    "babel-eslint": "10.1.0",
    "eslint": "8.27.0",
    "eslint-friendly-formatter": "4.0.1",
    "eslint-loader": "4.0.2",
    "eslint-plugin-vue": "9.7.0",
    "node-sass": "8.0.0",
    "replace": "1.2.2",
    "sass-loader": "10"
  }
}
