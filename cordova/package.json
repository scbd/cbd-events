{
    "name": "UNBiodiversityEvents",
    "displayName": "UNBiodiversityEvents",
    "version": "1.3.5",
    "description": "Cordova wrapper for CBD/UN Biodiversity Events",
    "scripts": {
        "platform:reload:browser": "cordova platform remove browser && cordova platform add browser",
        "platform:reload:ios": "cordova platform remove ios && cordova platform add ios",
        "platform:reload:android": "cordova platform remove android && cordova platform add android",
        "clear:cache": "rm package-lock.json && rm -rf node_modules && rm -rf plugins/* && rm -rf platforms/*",
        "release:ios": "appcenter codepush release -a SCBD/cbd-events-ios -c ./platforms/ios/www -d Production -t \"*\" --mandatory",
        "release:android": "appcenter codepush release -a SCBD/cbd-events-android -c ./platforms/android/app/src/main/assets/www -d Production -t \"*\" --mandatory"
    },
    "author": "Apache Cordova Team",
    "license": "Apache-2.0",
    "dependencies": {
        "code-push": "^3.0.1",
        "cordova-android": "^8.1.0",
        "cordova-browser": "^6.0.0",
        "cordova-ios": "^5.0.1",
        "cordova-plugin-appcenter-analytics": "^0.4.0",
        "cordova-plugin-appcenter-crashes": "^0.4.0",
        "cordova-plugin-appcenter-shared": "^0.4.0",
        "cordova-plugin-code-push": "^1.12.0",
        "cordova-plugin-compat": "^1.2.0",
        "cordova-plugin-device": "^2.0.3",
        "cordova-plugin-dialogs": "^2.0.2",
        "cordova-plugin-file": "4.3.3",
        "cordova-plugin-file-opener2": "^2.0.19",
        "cordova-plugin-file-transfer": "^1.6.3",
        "cordova-plugin-inappbrowser": "^3.0.0",
        "cordova-plugin-splashscreen": "^5.0.2",
        "cordova-plugin-statusbar": "^2.4.2",
        "cordova-plugin-whitelist": "^1.3.3",
        "cordova-plugin-zip": "^3.1.0"
    },
    "cordova": {
        "plugins": {
            "cordova-plugin-whitelist": {},
            "cordova-plugin-statusbar": {},
            "cordova-plugin-splashscreen": {},
            "cordova-plugin-file-opener2": {
                "ANDROID_SUPPORT_V4_VERSION": "27.+"
            },
            "cordova-plugin-inappbrowser": {},
            "cordova-plugin-code-push": {},
            "cordova-plugin-file": {},
            "cordova-plugin-file-transfer": {},
            "cordova-plugin-zip": {},
            "cordova-plugin-device": {}
        },
        "platforms": [
            "browser",
            "ios",
            "android"
        ]
    },
    "devDependencies": {
        "code-push-cli": "^2.1.9"
    }
}